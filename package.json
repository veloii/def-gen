{
  "name": "def-gen",
  "version": "0.1.0",
  "description": "Generate TypeScript definitions and validation schemas from JSON objects",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "keywords": [
    "typescript",
    "definitions",
    "schema",
    "zod",
    "valibot",
    "generator"
  ],
  "author": "Aria (veloii)",
  "license": "MIT",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "bin": {
    "def-gen": "./dist/cli.js"
  },
  "scripts": {
    "test": "vitest",
    "cli": "pnpm build && ./dist/cli.js",
    "build": "pkgroll"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "pkgroll": "^2.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "cleye": "^1.3.2"
  }
}
